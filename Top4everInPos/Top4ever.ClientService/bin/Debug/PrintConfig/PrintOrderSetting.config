<?xml version="1.0"?>
<PrintSetting xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Line Name="SingleLine" Text="-" StartPX="20%" EndPX="100%">
		<Font>
			<FontName>Microsoft Sans Serif</FontName>
			<FontSize>8</FontSize>
			<FontStyle>Bold</FontStyle>
		</Font>
	</Line>
    <Line Name="StarLine" Text="*" StartPX="0%" EndPX="100%">
        <Font>
            <FontName>Microsoft Sans Serif</FontName>
            <FontSize>8</FontSize>
            <FontStyle>Bold</FontStyle>
        </Font>
    </Line>
	<NormalConfig Name="ShopName" Prefix="" Align="Center">
		<Font>
			<FontName>Microsoft YaHei</FontName>
			<FontSize>15</FontSize>
			<FontStyle>Bold</FontStyle>
		</Font>
	</NormalConfig>
	<NormalConfig Name="DeskName" Prefix="桌号：" Align="Left">
		<Font>
			<FontName>Microsoft YaHei</FontName>
			<FontSize>12</FontSize>
			<FontStyle>Regular</FontStyle>
		</Font>
	</NormalConfig>
	<NormalConfig Name="PersonNum" Prefix="人数：" Align="Right">
		<Font>
			<FontName>Microsoft Sans Serif</FontName>
			<FontSize>9</FontSize>
			<FontStyle>Regular</FontStyle>
		</Font>
	</NormalConfig>
	<NormalConfig Name="PrintTime" Prefix="日期：" Align="Left">
		<Font>
			<FontName>Microsoft Sans Serif</FontName>
			<FontSize>9</FontSize>
			<FontStyle>Regular</FontStyle>
		</Font>
	</NormalConfig>
	<NormalConfig Name="EmployeeNo" Prefix="点单员：" Align="Left">
		<Font>
			<FontName>Microsoft Sans Serif</FontName>
			<FontSize>9</FontSize>
			<FontStyle>Regular</FontStyle>
		</Font>
	</NormalConfig>
	<NormalConfig Name="Cashier" Prefix="收银员：" Align="Right">
		<Font>
			<FontName>Microsoft Sans Serif</FontName>
			<FontSize>9</FontSize>
			<FontStyle>Regular</FontStyle>
		</Font>
	</NormalConfig>
	<NormalConfig Name="ShopAddress" Prefix="" Align="Center">
		<Font>
			<FontName>Microsoft YaHei</FontName>
			<FontSize>10</FontSize>
			<FontStyle>Regular</FontStyle>
		</Font>
	</NormalConfig>
	<NormalConfig Name="Telephone" Prefix="" Align="Center">
		<Font>
			<FontName>Microsoft YaHei</FontName>
			<FontSize>10</FontSize>
			<FontStyle>Regular</FontStyle>
		</Font>
	</NormalConfig>
    <DataListConfig ClassName="GoodsOrder">
        <ColumnHead Name="GoodsName" Text="品名" Align="0%">
            <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>10</FontSize>
                <FontStyle>Regular</FontStyle>
            </Font>
        </ColumnHead>
        <ColumnHead Name="GoodsNum" Text="数量" Align="45%">
            <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>10</FontSize>
                <FontStyle>Regular</FontStyle>
            </Font>
        </ColumnHead>
        <ColumnHead Name="SellPrice" Text="单价" Align="60%">
            <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>10</FontSize>
                <FontStyle>Regular</FontStyle>
            </Font>
        </ColumnHead>
        <ColumnHead Name="TotalSellPrice" Text="小计" Align="75%">
            <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>10</FontSize>
                <FontStyle>Regular</FontStyle>
            </Font>
        </ColumnHead>
        <ColumnHead Name="TotalDiscount" Text="折扣" Align="88%">
            <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>10</FontSize>
                <FontStyle>Regular</FontStyle>
            </Font>
        </ColumnHead>
        <Column Name="GoodsName" Align="0%">
            <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>8</FontSize>
                <FontStyle>Regular</FontStyle>
            </Font>
        </Column>
        <Column Name="GoodsNum" Align="45%">
            <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>8</FontSize>
                <FontStyle>Regular</FontStyle>
            </Font>
        </Column>
        <Column Name="SellPrice" Align="60%">
            <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>8</FontSize>
                <FontStyle>Regular</FontStyle>
            </Font>
        </Column>
        <Column Name="TotalSellPrice" Align="75%">
            <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>8</FontSize>
                <FontStyle>Regular</FontStyle>
            </Font>
        </Column>
        <Column Name="TotalDiscount" Align="90%">
            <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>8</FontSize>
                <FontStyle>Regular</FontStyle>
            </Font>
        </Column>
    </DataListConfig>
</PrintSetting>