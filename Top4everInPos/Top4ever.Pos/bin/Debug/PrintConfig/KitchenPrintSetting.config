<?xml version="1.0" encoding="utf-8" ?>
<PrintSetting xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <PrintConfig PaperType="76mm" Margin="12,12">
        <LineCollection>
            <Line Name="SingleLine" Text="-" StartPX="20%" EndPX="100%">
                <Font>
                    <FontName>Microsoft Sans Serif</FontName>
                    <FontSize>8</FontSize>
                    <FontStyle>Bold</FontStyle>
                    <ForeColor>Black</ForeColor>
                </Font>
            </Line>
            <Line Name="StarLine" Text="*" StartPX="0%" EndPX="100%">
                <Font>
                    <FontName>Microsoft Sans Serif</FontName>
                    <FontSize>8</FontSize>
                    <FontStyle>Bold</FontStyle>
                    <ForeColor>Black</ForeColor>
                </Font>
            </Line>
        </LineCollection>
        <NormalConfigCollection>
            <NormalConfig Name="BillTitle" Prefix="" Align="Center">
                <Font>
                    <FontName>Microsoft YaHei</FontName>
                    <FontSize>15</FontSize>
                    <FontStyle>Bold</FontStyle>
                    <ForeColor>Black</ForeColor>
                </Font>
            </NormalConfig>
            <NormalConfig Name="DeskName" Prefix="桌号：" Align="0%">
                <Font>
                    <FontName>Microsoft YaHei</FontName>
                    <FontSize>15</FontSize>
                    <FontStyle>Regular</FontStyle>
                    <ForeColor>Black</ForeColor>
                </Font>
            </NormalConfig>
            <NormalConfig Name="EatTypeName" Prefix="" Align="60%">
                <Font>
                    <FontName>Microsoft YaHei</FontName>
                    <FontSize>15</FontSize>
                    <FontStyle>Regular</FontStyle>
                    <ForeColor>Black</ForeColor>
                </Font>
            </NormalConfig>
            <NormalConfig Name="TaskTypeName" Prefix="" Align="80%">
                <Font>
                    <FontName>Microsoft YaHei</FontName>
                    <FontSize>15</FontSize>
                    <FontStyle>Regular</FontStyle>
                    <ForeColor>Black</ForeColor>
                </Font>
            </NormalConfig>
            <NormalConfig Name="PrintTime" Prefix="日期：" Align="Left">
                <Font>
                    <FontName>Microsoft Sans Serif</FontName>
                    <FontSize>9</FontSize>
                    <FontStyle>Regular</FontStyle>
                    <ForeColor>Black</ForeColor>
                </Font>
            </NormalConfig>
            <NormalConfig Name="PersonNum" Prefix="人数：" Align="Right">
                <Font>
                    <FontName>Microsoft Sans Serif</FontName>
                    <FontSize>9</FontSize>
                    <FontStyle>Regular</FontStyle>
                    <ForeColor>Black</ForeColor>
                </Font>
            </NormalConfig>
            <NormalConfig Name="EmployeeNo" Prefix="点单员：" Align="Left">
                <Font>
                    <FontName>Microsoft Sans Serif</FontName>
                    <FontSize>9</FontSize>
                    <FontStyle>Regular</FontStyle>
                    <ForeColor>Black</ForeColor>
                </Font>
            </NormalConfig>
        </NormalConfigCollection>
        <DataListConfigCollection>
            <DataListConfig ClassName="GoodsOrder">
                <ColumnHeadCollection>
                    <ColumnHead Name="GoodsNum" Text="数量" Align="0%">
                        <Font>
                            <FontName>Microsoft YaHei</FontName>
                            <FontSize>10</FontSize>
                            <FontStyle>Regular</FontStyle>
                            <ForeColor>Black</ForeColor>
                        </Font>
                    </ColumnHead>
                    <ColumnHead Name="GoodsName" Text="品名" Align="30%">
                        <Font>
                            <FontName>Microsoft YaHei</FontName>
                            <FontSize>10</FontSize>
                            <FontStyle>Regular</FontStyle>
                            <ForeColor>Black</ForeColor>
                        </Font>
                    </ColumnHead>
                    <ColumnHead Name="Unit" Text="单位" Align="80%">
                        <Font>
                            <FontName>Microsoft YaHei</FontName>
                            <FontSize>10</FontSize>
                            <FontStyle>Regular</FontStyle>
                            <ForeColor>Black</ForeColor>
                        </Font>
                    </ColumnHead>
                </ColumnHeadCollection>
                <ColumnCollection>
                    <Column Name="GoodsNum" Align="0%" IsRight="false">
                        <Font>
                            <FontName>Microsoft YaHei</FontName>
                            <FontSize>8</FontSize>
                            <FontStyle>Regular</FontStyle>
                            <ForeColor>Black</ForeColor>
                        </Font>
                    </Column>
                    <Column Name="GoodsName" Align="30%" IsRight="false">
                        <Font>
                            <FontName>Microsoft YaHei</FontName>
                            <FontSize>8</FontSize>
                            <FontStyle>Regular</FontStyle>
                            <ForeColor>Black</ForeColor>
                        </Font>
                    </Column>
                    <Column Name="Unit" Align="80%" IsRight="false">
                        <Font>
                            <FontName>Microsoft YaHei</FontName>
                            <FontSize>8</FontSize>
                            <FontStyle>Regular</FontStyle>
                            <ForeColor>Black</ForeColor>
                        </Font>
                    </Column>
                </ColumnCollection>
            </DataListConfig>
        </DataListConfigCollection>
    </PrintConfig>
</PrintSetting>