<?xml version="1.0"?>
<PrintSetting xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <PrintConfig PaperType="76mm" Margin="12,12">
      <LineCollection>
        <Line Name="SingleLine" Text="-" StartPX="20%" EndPX="100%">
          <Font>
            <FontName>Microsoft Sans Serif</FontName>
            <FontSize>8</FontSize>
            <FontStyle>Bold</FontStyle>
            <ForeColor>Black</ForeColor>
          </Font>
        </Line>
        <Line Name="StarLine" Text="*" StartPX="0%" EndPX="100%">
          <Font>
            <FontName>Microsoft Sans Serif</FontName>
            <FontSize>8</FontSize>
            <FontStyle>Bold</FontStyle>
            <ForeColor>Black</ForeColor>
          </Font>
        </Line>
      </LineCollection>
      <NormalConfigCollection>
        <NormalConfig Name="ShopName" Prefix="" Align="Center">
          <Font>
            <FontName>Microsoft YaHei</FontName>
            <FontSize>15</FontSize>
            <FontStyle>Bold</FontStyle>
            <ForeColor>Black</ForeColor>
          </Font>
        </NormalConfig>
        <NormalConfig Name="DeskName" Prefix="桌号：" Align="Left">
          <Font>
            <FontName>Microsoft YaHei</FontName>
            <FontSize>12</FontSize>
            <FontStyle>Regular</FontStyle>
            <ForeColor>Black</ForeColor>
          </Font>
        </NormalConfig>
        <NormalConfig Name="PersonNum" Prefix="人数：" Align="Right">
          <Font>
            <FontName>Microsoft Sans Serif</FontName>
            <FontSize>9</FontSize>
            <FontStyle>Regular</FontStyle>
            <ForeColor>Black</ForeColor>
          </Font>
        </NormalConfig>
        <NormalConfig Name="PrintTime" Prefix="日期：" Align="Left">
          <Font>
            <FontName>Microsoft Sans Serif</FontName>
            <FontSize>9</FontSize>
            <FontStyle>Regular</FontStyle>
            <ForeColor>Black</ForeColor>
          </Font>
        </NormalConfig>
        <NormalConfig Name="EmployeeNo" Prefix="点单员：" Align="Left">
          <Font>
            <FontName>Microsoft Sans Serif</FontName>
            <FontSize>9</FontSize>
            <FontStyle>Regular</FontStyle>
            <ForeColor>Black</ForeColor>
          </Font>
        </NormalConfig>
        <NormalConfig Name="Cashier" Prefix="收银员：" Align="Right">
          <Font>
            <FontName>Microsoft Sans Serif</FontName>
            <FontSize>9</FontSize>
            <FontStyle>Regular</FontStyle>
            <ForeColor>Black</ForeColor>
          </Font>
        </NormalConfig>
        <NormalConfig Name="ShopAddress" Prefix="" Align="Center">
          <Font>
            <FontName>Microsoft YaHei</FontName>
            <FontSize>10</FontSize>
            <FontStyle>Regular</FontStyle>
            <ForeColor>Black</ForeColor>
          </Font>
        </NormalConfig>
        <NormalConfig Name="Telephone" Prefix="" Align="Center">
          <Font>
            <FontName>Microsoft YaHei</FontName>
            <FontSize>10</FontSize>
            <FontStyle>Regular</FontStyle>
            <ForeColor>Black</ForeColor>
          </Font>
        </NormalConfig>
        <NormalConfig Name="ReceivableMoney" Prefix="应收金额：" Align="Right">
          <Font>
            <FontName>Microsoft YaHei</FontName>
            <FontSize>12</FontSize>
            <FontStyle>Regular</FontStyle>
            <ForeColor>Black</ForeColor>
          </Font>
        </NormalConfig>
        <NormalConfig Name="PaidInMoney" Prefix="实收金额：" Align="Right">
          <Font>
            <FontName>Microsoft YaHei</FontName>
            <FontSize>12</FontSize>
            <FontStyle>Regular</FontStyle>
            <ForeColor>Black</ForeColor>
          </Font>
        </NormalConfig>
        <NormalConfig Name="NeedChangePay" Prefix="找零金额：" Align="Right">
          <Font>
            <FontName>Microsoft YaHei</FontName>
            <FontSize>12</FontSize>
            <FontStyle>Regular</FontStyle>
            <ForeColor>Black</ForeColor>
          </Font>
        </NormalConfig>
      </NormalConfigCollection>
      <DataListConfigCollection>
        <DataListConfig ClassName="GoodsOrder">
          <ColumnHeadCollection>
            <ColumnHead Name="GoodsName" Text="品名" Align="0%">
              <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>10</FontSize>
                <FontStyle>Regular</FontStyle>
                <ForeColor>Black</ForeColor>
              </Font>
            </ColumnHead>
            <ColumnHead Name="GoodsNum" Text="数量" Align="45%">
              <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>10</FontSize>
                <FontStyle>Regular</FontStyle>
                <ForeColor>Black</ForeColor>
              </Font>
            </ColumnHead>
            <ColumnHead Name="SellPrice" Text="单价" Align="60%">
              <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>10</FontSize>
                <FontStyle>Regular</FontStyle>
                <ForeColor>Black</ForeColor>
              </Font>
            </ColumnHead>
            <ColumnHead Name="TotalSellPrice" Text="小计" Align="75%">
              <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>10</FontSize>
                <FontStyle>Regular</FontStyle>
                <ForeColor>Black</ForeColor>
              </Font>
            </ColumnHead>
            <ColumnHead Name="TotalDiscount" Text="折扣" Align="88%">
              <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>10</FontSize>
                <FontStyle>Regular</FontStyle>
                <ForeColor>Black</ForeColor>
              </Font>
            </ColumnHead>
          </ColumnHeadCollection>
          <ColumnCollection>
            <Column Name="GoodsName" Align="0%" IsRight="false">
              <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>8</FontSize>
                <FontStyle>Regular</FontStyle>
                <ForeColor>Black</ForeColor>
              </Font>
            </Column>
            <Column Name="GoodsNum" Align="45%" IsRight="false">
              <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>8</FontSize>
                <FontStyle>Regular</FontStyle>
                <ForeColor>Black</ForeColor>
              </Font>
            </Column>
            <Column Name="SellPrice" Align="72%" IsRight="true">
              <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>8</FontSize>
                <FontStyle>Regular</FontStyle>
                <ForeColor>Black</ForeColor>
              </Font>
            </Column>
            <Column Name="TotalSellPrice" Align="86%" IsRight="true">
              <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>8</FontSize>
                <FontStyle>Regular</FontStyle>
                <ForeColor>Black</ForeColor>
              </Font>
            </Column>
            <Column Name="TotalDiscount" Align="100%" IsRight="true">
              <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>8</FontSize>
                <FontStyle>Regular</FontStyle>
                <ForeColor>Red</ForeColor>
              </Font>
            </Column>
          </ColumnCollection>
        </DataListConfig>

        <DataListConfig ClassName="PayingGoodsOrder">
          <ColumnHeadCollection>
            <ColumnHead Name="PayoffName" Text="支付名称" Align="0%">
              <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>10</FontSize>
                <FontStyle>Regular</FontStyle>
                <ForeColor>Black</ForeColor>
              </Font>
            </ColumnHead>
            <ColumnHead Name="PayoffMoney" Text="支付金额" Align="40%">
              <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>10</FontSize>
                <FontStyle>Regular</FontStyle>
                <ForeColor>Black</ForeColor>
              </Font>
            </ColumnHead>
            <ColumnHead Name="NeedChangePay" Text="找零金额" Align="70%">
              <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>10</FontSize>
                <FontStyle>Regular</FontStyle>
                <ForeColor>Black</ForeColor>
              </Font>
            </ColumnHead>
          </ColumnHeadCollection>
          <ColumnCollection>
            <Column Name="PayoffName" Align="0%" IsRight="false">
              <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>8</FontSize>
                <FontStyle>Regular</FontStyle>
                <ForeColor>Black</ForeColor>
              </Font>
            </Column>
            <Column Name="PayoffMoney" Align="60%" IsRight="true">
              <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>8</FontSize>
                <FontStyle>Regular</FontStyle>
                <ForeColor>Black</ForeColor>
              </Font>
            </Column>
            <Column Name="NeedChangePay" Align="90%" IsRight="true">
              <Font>
                <FontName>Microsoft YaHei</FontName>
                <FontSize>8</FontSize>
                <FontStyle>Regular</FontStyle>
                <ForeColor>Black</ForeColor>
              </Font>
            </Column>
          </ColumnCollection>
        </DataListConfig>
      </DataListConfigCollection>
    </PrintConfig>
</PrintSetting>