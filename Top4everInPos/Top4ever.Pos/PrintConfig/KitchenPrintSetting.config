<?xml version="1.0" encoding="utf-8" ?>
<PrintSetting xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <PrintConfig PaperType="76mm" Margin="12,12">
        <LineCollection>
            <Line Name="SingleLine" Text="-" StartPX="0%" EndPX="100%">
                <Font>
                    <FontName>Microsoft Sans Serif</FontName>
                    <FontSize>8</FontSize>
                    <FontStyle>Bold</FontStyle>
                    <ForeColor>Black</ForeColor>
                </Font>
            </Line>
            <Line Name="StarLine" Text="*" StartPX="0%" EndPX="100%">
                <Font>
                    <FontName>Microsoft Sans Serif</FontName>
                    <FontSize>8</FontSize>
                    <FontStyle>Bold</FontStyle>
                    <ForeColor>Black</ForeColor>
                </Font>
            </Line>
        </LineCollection>
        <NormalConfigCollection>
            <NormalConfig Name="BillTitle" Prefix="" Width="100%" Align="Center">
                <Font>
                    <FontName>Microsoft YaHei</FontName>
                    <FontSize>15</FontSize>
                    <FontStyle>Bold</FontStyle>
                    <ForeColor>Black</ForeColor>
                </Font>
            </NormalConfig>
            <NormalConfig Name="DeskName" Prefix="桌号：" Width="50%" Align="Left">
                <Font>
                    <FontName>Microsoft YaHei</FontName>
                    <FontSize>15</FontSize>
                    <FontStyle>Regular</FontStyle>
                    <ForeColor>Black</ForeColor>
                </Font>
            </NormalConfig>
            <NormalConfig Name="EatTypeName" Prefix="" Width="20%" Align="Left">
                <Font>
                    <FontName>Microsoft YaHei</FontName>
                    <FontSize>15</FontSize>
                    <FontStyle>Regular</FontStyle>
                    <ForeColor>Black</ForeColor>
                </Font>
            </NormalConfig>
            <NormalConfig Name="TaskTypeName" Prefix="" Width="30%" Align="Left">
                <Font>
                    <FontName>Microsoft YaHei</FontName>
                    <FontSize>15</FontSize>
                    <FontStyle>Regular</FontStyle>
                    <ForeColor>Black</ForeColor>
                </Font>
            </NormalConfig>
            <NormalConfig Name="PrintTime" Prefix="日期：" Width="100%" Align="Left">
                <Font>
                    <FontName>Microsoft Sans Serif</FontName>
                    <FontSize>9</FontSize>
                    <FontStyle>Regular</FontStyle>
                    <ForeColor>Black</ForeColor>
                </Font>
            </NormalConfig>
            <NormalConfig Name="PersonNum" Prefix="人数：" Width="50%" Align="Left">
                <Font>
                    <FontName>Microsoft Sans Serif</FontName>
                    <FontSize>9</FontSize>
                    <FontStyle>Regular</FontStyle>
                    <ForeColor>Black</ForeColor>
                </Font>
            </NormalConfig>
            <NormalConfig Name="EmployeeNo" Prefix="点单员：" Width="50%" Align="Left">
                <Font>
                    <FontName>Microsoft Sans Serif</FontName>
                    <FontSize>9</FontSize>
                    <FontStyle>Regular</FontStyle>
                    <ForeColor>Black</ForeColor>
                </Font>
            </NormalConfig>
        </NormalConfigCollection>
        <DataListConfigCollection>
            <DataListConfig ClassName="GoodsOrder">
                <ColumnHeadCollection>
                    <ColumnHead Name="GoodsNum" Text="数量" Width="15%" Align="Center">
                        <Font>
                            <FontName>Microsoft YaHei</FontName>
                            <FontSize>10</FontSize>
                            <FontStyle>Regular</FontStyle>
                            <ForeColor>Black</ForeColor>
                        </Font>
                    </ColumnHead>
                    <ColumnHead Name="GoodsName" Text="品名" Width="65%" Align="Center">
                        <Font>
                            <FontName>Microsoft YaHei</FontName>
                            <FontSize>10</FontSize>
                            <FontStyle>Regular</FontStyle>
                            <ForeColor>Black</ForeColor>
                        </Font>
                    </ColumnHead>
                    <ColumnHead Name="Unit" Text="单位" Width="20%" Align="Center">
                        <Font>
                            <FontName>Microsoft YaHei</FontName>
                            <FontSize>10</FontSize>
                            <FontStyle>Regular</FontStyle>
                            <ForeColor>Black</ForeColor>
                        </Font>
                    </ColumnHead>
                </ColumnHeadCollection>
                <ColumnCollection>
                    <Column Name="GoodsNum" Width="15%" Align="Center">
                        <Font>
                            <FontName>Microsoft YaHei</FontName>
                            <FontSize>10</FontSize>
                            <FontStyle>Regular</FontStyle>
                            <ForeColor>Black</ForeColor>
                        </Font>
                    </Column>
                    <Column Name="GoodsName" Width="65%" Align="Left">
                        <Font>
                            <FontName>Microsoft YaHei</FontName>
                            <FontSize>10</FontSize>
                            <FontStyle>Regular</FontStyle>
                            <ForeColor>Black</ForeColor>
                        </Font>
                    </Column>
                    <Column Name="Unit" Width="20%" Align="Center">
                        <Font>
                            <FontName>Microsoft YaHei</FontName>
                            <FontSize>10</FontSize>
                            <FontStyle>Regular</FontStyle>
                            <ForeColor>Black</ForeColor>
                        </Font>
                    </Column>
                </ColumnCollection>
            </DataListConfig>
        </DataListConfigCollection>
    </PrintConfig>
</PrintSetting>